{
  "name": "gatsby-blog-typescript-jest-tailwind",
  "private": true,
  "description": "Starter site for a simple Gatsby blog with TypeScript",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages --dotfiles -d public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "gatsby": "^3.0.4",
    "gatsby-plugin-catch-links": "^3.5.0",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sharp": "^3.5.0",
    "gatsby-plugin-sitemap": "^4.1.0",
    "gatsby-plugin-verify-brave": "^1.0.5",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^5.2.0",
    "gatsby-remark-katex": "^5.0.0",
    "gatsby-remark-responsive-iframe": "^3.2.0",
    "gatsby-remark-smartypants": "^4.2.0",
    "gatsby-remark-vscode": "^3.2.1",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.2.0",
    "katex": "^0.13.2",
    "monokai-operator-theme": "github:markfknight/monokai-operator",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.5.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-testing-library": "^4.6.0",
    "gatsby-plugin-postcss": "^4.6.0",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.15",
    "prettier": "2.2.1",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markfknight/gatsby-blog-typescript-jest-tailwind"
  },
  "bugs": {
    "url": "https://github.com/markfknight/gatsby-blog-typescript-jest-tailwind/issues"
  }
}
